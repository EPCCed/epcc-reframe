comment           : 3x3 sapphire surface benchmark (v1.1)
task              : SinglePoint
cut_off_energy    : 400 eV
print_clock       : T
elec_energy_tol   : 1.5e-2 eV
xc_functional     : pw91
metals_method     : dm
mixing_scheme     : pulay
finite_basis_corr : 0
smearing_width    : 0.1 eV
nup               : 648
ndown             : 648
nextra_bands      : 120
rand_seed         : 1
fix_occupancy     : f
spin_fix          : -1

! Select optimisation choices for max. speed
num_dump_cycles   : 0
opt_strategy_bias : +3
page_wvfns        : 0

popn_calculate    : false

! Uncomment to use SMP optimisations in FFT
! (6 processes per SMP block in this example)
num_proc_in_smp   : 4

